  <body>
    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-8 div-background">
        </div>
        <div class="col-sm-12 col-md-12 col-lg-4 col-xl-4 div-formulario">
          <form method="POST" action="/cadastro">
            <img src="/images/logo-branco.png" alt="logo" class="logo">
            <h1 class="titulo">Cadastrar</h1>
              <%- include('alerts') %>
            <div class="form-group">
              <label for="email" class="label">Email</label>
              <input type="email" class="form-control" id="email" aria-describedby="emailHelp" name="email" required>
            </div>
            <div class="form-group">
              <label for="senha" class="label">Senha</label>
              <input type="password" class="form-control" id="senha" name="senha" required>
            </div>
            <button type="submit" class="btn btn-cadastrar">Cadastrar</button>
          </form>
        </div>
      </div>
    </div>