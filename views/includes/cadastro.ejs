  <body>
    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-8 div-background">
        </div>
        <div class="col-sm-12 col-md-12 col-lg-4 col-xl-4 div-formulario">
          <form action="/cadastro" method="POST">
            <img src="/images/logo-branco.png" alt="logo" class="logo">
            <h1 class="titulo">Cadastrar</h1>
            <%if(error != null){%>
              <div class="alert alert-danger">
              <h4><strong>Erros</strong></h4>
              <ul>
                <% for(let erro of error){ %> 
                  <li><%=erro.msg%> </li>
                <% } %> 
              </ul>
              </div>
            <%}%> 
            <div class="form-group">
              <label for="email" class="label">Email</label>
              <input type="text" class="form-control" id="email" aria-describedby="emailHelp" name="email">
            </div>
            <div class="form-group">
              <label for="senha" class="label">Senha</label>
              <input type="password" class="form-control" id="senha" name="senha">
            </div>
            <button type="submit" class="btn btn-cadastrar">Cadastrar</button>
          </form>
        </div>
      </div>
    </div>